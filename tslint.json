{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-console": {
      "severity": "warning"
    },
    "no-unused-variable": {
      "severity": "warning"
    },
    "jsx-no-lambda": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "member-ordering": false
  }
}
